                                {
                                    <div data-thumb="@Url.Content("~/admin/images/" + image.Name)" class="flex-active-slide" data-thumb-alt aria-hidden="true"
                                         style="width: 355.188px; margin-right: 0px; float: left; display: block;">
                                        <a href="@Url.Content("~/admin/images/"+image.Name)">
                                            <img src="@Url.Content("~/admin/images/"+image.Name)" alt="" data-caption=""
                                                 data-src="@Url.Content("~/admin/images/" + image.Name)" data-large_image="@Url.Content("~/client/images/" + image.Name)"
